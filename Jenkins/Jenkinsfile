pipeline {
    agent any
    stages {
        stage('---change dir---') {
            steps {
                dir('project') {
                    bat 'cd'
                }
            }
        }
        stage('---clean---') {
                dir('project') {
                    bat 'cd'
                }
            steps {
                bat "mvn clean"
            }
        }
        stage('--test--') {
            steps {
                bat "mvn test"
            }
        }
        stage('--package--') {
            steps {
                bat "mvn package"
            }
        }
    }
}
