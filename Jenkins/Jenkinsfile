pipeline {
    agent any
    stages {
       stage('---clean---') {
            dir("project") {
                steps {
                    bat "mvn clean"
                }
            }
        }
        stage('--test--') {
            steps {
                bat "mvn test"
            }
        }
        stage('--package--') {
            steps {
                bat "mvn package"
            }
        }
    }
}
